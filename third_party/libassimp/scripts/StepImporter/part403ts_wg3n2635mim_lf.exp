	WR3 : SIZEOF (TYPEOF (SELF.related_draughting_callout) *
       ['AP203_CONFIGURATION_CONTROLLED_3D_DESIGN_OF_MECHANICAL_PARTS_AND_ASSEMBLIES_MIM_LF.ANGULAR_DIMENSION',
