    if test -n "$PKG_CONFIG" && \
    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"libdrm >= 1.4.82\""; } >&5
  ($PKG_CONFIG --exists --print-errors "libdrm >= 1.4.82") 2>&5
