	float s = sin(input.instanceRot.x + ubo.locSpeed);
	float c = cos(input.instanceRot.x + ubo.locSpeed);

	float3x3 mx = { c, -s, 0.0,
					s, c, 0.0,
					0.0, 0.0, 1.0 };

	// rotate around y
	s = sin(input.instanceRot.y + ubo.locSpeed);
	c = cos(input.instanceRot.y + ubo.locSpeed);

	float3x3 my = { c, 0.0, -s,
					0.0, 1.0, 0.0,
					s, 0.0, c };

	// rot around z
